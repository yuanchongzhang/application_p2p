<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorWhite"

    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_01"
        android:layout_width="match_parent"
        android:layout_height="68px"
        android:orientation="horizontal"
        android:tag="(0,68)">

        <TextView
            android:id="@+id/tv_01"
            android:layout_width="80px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(80,0,23)"
            android:text="@string/invest_vp_01"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />

        <TextView
            android:id="@+id/tv_02"
            android:layout_width="110px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(110,0,23)"
            android:text="@string/invest_vp_02"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />

        <TextView
            android:id="@+id/tv_03"
            android:layout_width="140px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(140,0,23)"
            android:text="投标金额"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />

        <TextView
            android:id="@+id/tv_04"
            android:layout_width="130px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(130,0,23)"
            android:text="@string/invest_vp_04"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />

        <TextView
            android:id="@+id/tv_05"
            android:layout_width="164px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(164,0,23)"
            android:text="投标利息"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />

        <TextView
            android:id="@+id/tv_06"
            android:layout_width="200px"
            android:layout_height="match_parent"
            android:gravity="center"
            android:tag="(200,0,23)"
            android:text="投标时间"
            android:textColor="@color/colorBlack"
            android:textSize="23px" />
    </LinearLayout>
</LinearLayout>